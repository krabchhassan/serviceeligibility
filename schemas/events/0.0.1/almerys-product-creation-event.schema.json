{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "Création produit Almerys",
  "description": "Création d'un produit Almerys",
  "additionalProperties": false,
  "properties": {
    "creationDate": {
      "type": "string",
      "description": "Date de création (avec timezone)"
    },
    "userCreation": {
      "type": "string",
      "description": "Utilisateur créant le paramétrage"
    },
    "code": {
      "type": "string",
      "description": "Code du référentiel"
    },
    "description": {
      "type": "string",
      "description": "Description"
    },
    "combinationList": {
      "type": "array",
      "items": {
        "$ref": "#/components/CombiAjout"
      },
      "description": "La liste des combinaisons"
    }
  },
  "required": [
    "code",
    "creationDate",
    "description"
  ],
  "components":{
    "CombiAjout": {
      "type": "object",
      "properties": {
        "numero": {
          "type": "string",
          "description": "Le numéro de la combinaison"
        },
        "debut": {
          "type": "string",
          "description": "La date de debut de la combinaison"
        },
        "fin": {
          "type": "string",
          "description": "La date de fin de la combinaison"
        },
        "finPrecedente": {
          "type": "string",
          "description": "La date de fin precedente de la combinaison"
        },
        "gtAjoutees": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "description": "Les GTs ajoutées"
        },
        "lotAjoutes": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "description": "Les lots ajoutés"
        }
      }
    }
  },
  "$id": "Insurance.serviceeligibility.almerys.product.creation.event/0.0.1"
}
