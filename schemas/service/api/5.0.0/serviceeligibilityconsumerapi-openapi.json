          "rangNaissance": {
            "type": "string",
            "description": "Rang de naissance de l'assuré"
          },
          "numeroPersonne": {
            "type": "string",
            "description": "Numéro de personne"
          },
          "refExternePersonne": {
            "type": "string",
            "description": "Référence externe de la personne"
          }
        }
      },
      "Nir": {
        "required": [
          "cle",
          "code"
        ],
        "type": "object",
        "description": "NIR",
        "properties": {
          "code": {
            "type": "string",
            "description": "Code du NIR"
          },
          "cle": {
            "type": "string",
            "description": "Clé du NIR"
          }
        }
      },
      "AffiliationsRO": {
        "required": [
          "nir",
          "periode"
        ],
        "type": "object",
        "properties": {
          "nir": {
            "$ref": "#/components/schemas/Nir"
          },
          "rattachementRO": {
            "$ref": "#/components/schemas/RattachementRO"
          },
          "periode": {
            "$ref": "#/components/schemas/Periode"
          }
        }
      },
      "RattachementRO": {
        "type": "object",
        "description": "Rattachement ",
        "properties": {
