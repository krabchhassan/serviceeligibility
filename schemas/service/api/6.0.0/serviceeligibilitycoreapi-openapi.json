            "schema": {
              "type": "string"
            }
          },
          {
            "in": "query",
            "name": "debutPeriodeSoin",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "in": "query",
            "name": "finPeriodeSoin",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "in": "query",
            "name": "nir",
            "required": false,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ContratV6"
                }
              }
            }
          },
          "400": {
            "description": "Veuillez renseigner le NIR ou le n° d'adhérent."
          },
          "404": {
            "description": "Les éléments fournis ne permettent pas d'identifier un bénéficiaire."
          }
        },
        "tags": [
          "service-prestation-controller-v-6"
        ]
      }
    },
    "/v6/servicePrestations/search": {
      "get": {
        "operationId": "searchPrestationsV6",
        "parameters": [
          {
            "in": "query",
            "name": "idDeclarant",
