          },
          "numeroExterne": {
            "type": "string",
            "description": "Identificant complémentaire pour le contrat collectif"
          },
          "identifiantCollectivite": {
            "type": "string",
            "description": "Identifiant de la collectivité du contrat collectif dans le backoffice externe"
          },
          "raisonSociale": {
            "type": "string",
            "description": "Raison sociale du contrat collectif"
          },
          "siret": {
            "type": "string",
            "description": "Siret du contrat collectif dans le backoffice externe"
          },
          "groupePopulation": {
            "type": "string",
            "description": "Groupe de population du contrat collectif"
          }
        }
      },
      "ContexteTPV6": {
        "type": "object",
        "properties": {
          "isCartePapierAEditer": {
            "type": "boolean",
            "description": "Booléen indiquant si la carte est à éditer"
          },
          "periodesDroitsCarte": {
            "$ref": "#/components/schemas/PeriodesDroitsCarte"
          },
          "dateRestitutionCarte": {
            "type": "string",
            "description": "Date de restitution de la carte papier"
          },
          "isCarteDematerialisee": {
            "type": "boolean",
            "description": "Booléen indiquant si il y a une carte dématerialisée"
          },
          "isCartePapier": {
            "type": "boolean",
            "description": "Booléen indiquant si il y a une carte papier"
          }
        }
      },
      "PeriodesDroitsCarte": {
        "required": [
          "debut",
          "fin"
        ],
        "type": "object",
        "description": "Période de validité des droits carte",
        "properties": {
          "debut": {
            "type": "string",
            "description": "Date de début de la période de droits carte"
          },
          "fin": {
            "type": "string",
            "description": "Date de fin de la période de droits carte"
          }
        }
      }
    }
  }
