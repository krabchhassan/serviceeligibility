          },
          "codeOc": {
            "type": "string"
          },
          "periodesContratCMUOuvert": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/PeriodeContratCMUOuvert"
            }
          },
          "nomFichierOrigine": {
            "type": "string"
          },
          "critereSecondaireDetaille": {
            "type": "string",
            "description": "Information permettant de découper la population (notion de portefeuille dans Organisation)"
          },
          "critereSecondaire": {
            "type": "string",
            "description": "Valeur inscrite au niveau de l’entête de l’attestation papier délivrée aux assurés issue du SI du backoffice"
          },
          "isContratIndividuel": {
            "type": "boolean",
            "required": true,
            "description": "Booléen permettant de définir si le contrat est en gestion individuelle"
          },
          "gestionnaire": {
            "type": "string",
            "required": true,
            "description": "Code du gestionnaire gérant le contrat. Valeur non contrôlée  (exemple : Adhérent, Gestionnaire, Entreprise)"
          },
          "contratCollectif": {
            "$ref": "#/components/schemas/ContratCollectif"
          },
          "contexteTiersPayant": {
            "$ref": "#/components/schemas/ContexteTPV6"
          },
          "qualification": {
            "type": "string",
            "required": true,
            "description": "Valeur permettant de qualifier le contrat. Gestion des contrats de base uniquement",
            "enum": [
              "B"
            ]
          },
          "ordrePriorisation": {
            "type": "string",
            "required": true,
            "description": "Ordre de priorisation du contrat. Utilisable en cas de multi contrats sinon valorisé à '1'"
          }
        }
      },
      "PeriodeSuspension": {
        "required": [
          "periode",
          "typeSuspension",
          "motifSuspension"
        ],
        "type": "object",
        "properties": {
          "periode": {
            "items": {
              "$ref": "#/components/schemas/Periode"
            },
            "description": "Période pendant laquelle le contrat est suspendu"
          },
          "typeSuspension": {
            "type": "string",
            "enum": [
              "Provisoire",
              "Definitif"
            ],
            "description": "Indique le type de suspension"
          },
          "motifSuspension": {
            "type": "string",
            "enum": [
              "NON_PAIEMENT_COTISATIONS",
              "PORTABILITE_NON_JUSTIFIEE"
            ],
            "description": "Motif de la suspension"
          },
          "motifLeveeSuspension": {
            "type": "string",
            "description": "Motif de la levée de suspension"
          }
        }
      },
      "PeriodeContratCMUOuvert": {
        "required": [
          "code",
          "periode"
        ],
        "type": "object",
        "properties": {
          "code": {
            "type": "string"
          },
          "periode": {
            "$ref": "#/components/schemas/Periode"
          }
        }
      },
      "ContratCollectif": {
        "required": [
          "numero",
          "identifiantCollectivite",
          "raisonSociale",
          "groupePopulation"
