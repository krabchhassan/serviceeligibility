<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<wsdl:definitions xmlns:ns="http://ws.cegedimassurance.com/servicestarificationprescriptionchiffree/wsdl"
	xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	xmlns:http="http://schemas.xmlsoap.org/wsdl/http/" xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:commun="http://norme.cegedimassurances.com/commun" xmlns:tarification-prescription-chiffree="http://norme.cegedimassurances.com/tarification-prescription-chiffree" 
	targetNamespace="http://ws.cegedimassurance.com/servicestarificationprescriptionchiffree/wsdl">
	<wsdl:types>
		<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
			targetNamespace="http://ws.cegedimassurance.com/servicestarificationprescriptionchiffree/wsdl">
		</xsd:schema>
		<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
			<xsd:import namespace="http://norme.cegedimassurances.com/tarification-prescription-chiffree"	schemaLocation="../xsd/tarification-prescription-chiffree/requete_tarification.xsd"/>
			<xsd:import namespace="http://norme.cegedimassurances.com/tarification-prescription-chiffree"	schemaLocation="../xsd/tarification-prescription-chiffree/reponse_tarification.xsd"/>
		</xsd:schema>
	</wsdl:types>

	<wsdl:message name="getTarifRequest">
		<wsdl:part name="requete_tarification" type="tarification-prescription-chiffree:requete_tarification" />
	</wsdl:message>

	<wsdl:message name="getTarifResponse">
		<wsdl:part name="reponse_tarification" type="tarification-prescription-chiffree:reponse_tarification" />
	</wsdl:message>

	<wsdl:portType name="ServicesTarificationprescriptionchiffree">
		<wsdl:operation name="getDevis">
			<wsdl:documentation>Appel du service de devis de la tarification</wsdl:documentation>
			<wsdl:input message="ns:getTarifRequest" />
			<wsdl:output message="ns:getTarifResponse" />
		</wsdl:operation>
		<wsdl:operation name="getPec">
			<wsdl:documentation>Appel du service prise en charge de la tarification. Dans le header de la réponse doit être renseigné le numéro d'accréditation.</wsdl:documentation>
			<wsdl:input message="ns:getTarifRequest" />
			<wsdl:output message="ns:getTarifResponse" />
		</wsdl:operation>
	</wsdl:portType>

	<wsdl:binding name="ServicesTarificationBinding" type="ns:ServicesTarificationprescriptionchiffree">
		<soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http" />
		<wsdl:operation name="getDevis">
			<soap:operation
				soapAction="http://ws.cegedimassurance.com/servicestarificationprescriptionchiffree/wsdl/getDevis" />
			<wsdl:input>
				<soap:body use="literal" />
			</wsdl:input>
			<wsdl:output>
				<soap:body use="literal" />
			</wsdl:output>
		</wsdl:operation>
		<wsdl:operation name="getPec">
			<soap:operation
				soapAction="http://ws.cegedimassurance.com/servicestarificationprescriptionchiffree/wsdl/getPec" />
			<wsdl:input>
				<soap:body use="literal" />
			</wsdl:input>
			<wsdl:output>
				<soap:body use="literal" />
			</wsdl:output>
		</wsdl:operation>
	</wsdl:binding>

	<wsdl:service name="FSIQServicesTarificationChiffree">
		<wsdl:port binding="ns:ServicesTarificationBinding" name="ServicesTarificationPort">
			<soap:address
				location="http://fsiq.services-fm.net/wa-tarification/services/services-tarification-prescription-chiffree/" />
		</wsdl:port>
	</wsdl:service>
</wsdl:definitions>
